# CMake @_ep_name@ @_action@ file

# Now run the @_action@ script
execute_process(
  COMMAND sh @_script@
    RESULT_VARIABLE result
    WORKING_DIRECTORY "@_work_dir@"
)

if (result)
  set(msg "@_ep_name@ @_action@ command failed")
  foreach(arg IN LISTS command )
    set(msg "${msg} '${arg}'")
  endforeach()
  message(FATAL_ERROR ${msg})
endif()  

              
